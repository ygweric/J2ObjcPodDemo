source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
use_frameworks!


# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY START - can be moved as a block
def j2objc_j2objc
    pod 'j2objc-j2objc-debug', :configuration => ['Debug'], :path => './j2objcOutputs'
end
# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY END

target 'J2ObjcPodDemo' do
    platform :ios, '8.3'
    j2objc_j2objc
end
